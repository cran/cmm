citHeader("To cite package 'cmm' in publications use:")

citEntry(entry = "Article",
         title        = "cmm: An R package for marginal modelling of categorical data (version 0.2)",
         author       = personList(as.person("Wicher P. Bergsma"),as.person("L. Andries van der Ark")),
         journal      = "Unpublished manuscript",
         year         = "2010",
         volume       = "",
         number       = "",
         pages        = "",
         url          = "http://www.cmm.st",

         textVersion  = paste("Wicher P. Bergsma and L. Andries van der Ark (2010).",
               "cmm: An R package for marginal modelling of categorical data (version 0.2),",
               "Unpublished manuscript",
             "URL http://www.cmm.st")
)
